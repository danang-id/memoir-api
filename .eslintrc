{
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier"
	],
	"plugins": [
		"@typescript-eslint",
		"prettier"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"env": {
		"browser": true,
		"es2021": true
	},
	"rules": {
		"@typescript-eslint/no-empty-function": "warn",
		"prettier/prettier": "error"
	},
	"ignorePatterns": [
		"node_modules/",
		"build/",
		"dist/",
		".cache/",
		".tmp/"
	]
}
